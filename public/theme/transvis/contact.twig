{% extends '@theme/partials/_master.twig' %}

{% block stylesheet %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('css/contact.css') }}">
{% endblock stylesheet %}

{% block javascript %}
    {{ parent() }}
{%  endblock javascript %}

{% block main %}
    <div class="row box-top text-center">
        <h1 class="mb-4">{{ record.title }}</h1>
        {{ record.teaser }}
    </div>
    <div class="row box-bottom p-4" style="background-image: url({{ asset('images/bgRed.png') }}); background-size: cover;">
        <div class="col-7">
            {{ boltform('contact') }}
        </div>
        <div class="col-5">
            <iframe width="480" height="370" id="gmap_canvas" src="https://maps.google.com/maps?q=orl%C3%A9ans%2045000&t=&z=13&ie=UTF8&iwloc=&output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe>
        </div>
    </div>
{% endblock main %}
